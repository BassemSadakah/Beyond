        box-sizing: content-box;
        box-sizing: border-box;

    }
</style>
<div class="container">

    <div class="row justify-content-md-center">
        <%for(var x =1;x<25;x++){%>
        <div class="col col-lg-3 col-md-4 col-sm-6 col-xs-12 ">
            <div class="square">

                <img class="image " src="img/stem-photos/<%=x%>.jpg" onerror="this.src=\"img/stem-photos/<%=x%>.png\"">

            </div>
        </div>
        <%}%>
        </div>
 </div>



