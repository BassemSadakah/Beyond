<style>
    body {
        background-color: #fafafa;
    }

    .image {
        display: block;
        max-width: 290px;
        max-height: 293px;
        width: auto;
        height: auto;
        display: block;
        margin: auto;
        position: absolute;
       top:0;
       left: 0px;
       right: 0;
       bottom: 0;
